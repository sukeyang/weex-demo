{
  "dependencies" : {
    "vue" : "^2.1.10",
    "vue-router" : "^2.1.1",
    "vuex" : "^2.1.1",
    "vuex-router-sync" : "^4.0.1",
    "weex-vue-render" : "^0.2.0"
  },
